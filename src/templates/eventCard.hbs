{{#each this._embedded.events}}
<li class="event-item js-event-item" id="{{id}}">
        <div class="event" id="{{id}}">
            <img src="{{images.5.url}}" alt="{{name}}" class="event-img" id="{{id}}">
            <h2 class="event-name element" id="{{id}}">{{name}}</h2>
            <p class="event-date element" id="{{id}}">{{dates.start.localDate}}</p>
            <div class="location" id="{{id}}">
                <svg class="icon-location" id="{{id}}">
                    <path 
                        d="M3 0C1.34581 0 0 1.40339 0 3.12836C0 5.29645 3.00295 9 3.00295 9C3.00295 9 6 5.18983 6 3.12836C6 1.40339 4.65424 0 3 0ZM3.90516 4.04434C3.65558 4.30455 3.32781 4.43469 3 4.43469C2.67224 4.43469 2.34437 4.30455 2.09489 4.04434C1.59577 3.52392 1.59577 2.67709 2.09489 2.15662C2.33658 1.90448 2.65807 1.76561 3 1.76561C3.34193 1.76561 3.66337 1.90453 3.90516 2.15662C4.40428 2.67709 4.40428 3.52392 3.90516 4.04434Z"
                        fill="white" />
                </svg>
                <p class="event-place element" id="{{id}}">{{promoter.name}}</p>
            </div>
        </div>
</li>
{{/each}}