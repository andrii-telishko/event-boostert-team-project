{{#each this._embedded.events}}
<li class="event-item js-event-item" id="{{id}}">
        <div class="event" id="{{id}}">
            <img src="{{images.5.url}}" alt="{{name}}" class="event-img" id="{{id}}">
            <h2 class="event-name element" id="{{id}}">{{name}}</h2>
            <p class="event-date element" id="{{id}}">{{dates.start.localDate}}</p>
            <div class="location" id="{{id}}">
                <svg class="icon-location" id="{{id}}">
                    <use href="./images/svg/main.svg#icon-local">
                    </use>
                </svg>
                <p class="event-place element" id="{{id}}">{{promoter.name}}</p>
            </div>
        </div>
</li>
{{/each}}